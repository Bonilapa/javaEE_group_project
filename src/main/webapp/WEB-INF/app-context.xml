<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:context = "http://www.springframework.org/schema/context"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>
    <context:component-scan base-package = "org.innopolis.javaEE" />

    <bean class = "org.springframework.web.servlet.view.InternalResourceViewResolver">
        <property name = "prefix" value = "/" />
        <property name = "suffix" value = ".jsp" />
    </bean>

    <bean id="exportService" class="org.innopolis.javaEE.fileIO.service.implementations.ExportServiceImpl">

    </bean>

    <bean id="employeeDAO" class="org.innopolis.javaEE.dataService.impl.EmployeeDAOImpl">

    </bean>

    <bean id="loginService" class="org.innopolis.javaEE.aureg.services.impl.LoginServiceImpl"/>
    <bean id="registerService" class="org.innopolis.javaEE.aureg.services.impl.RegisterServiceImpl"/>
    <bean id="userRightsEditService" class="org.innopolis.javaEE.aureg.services.impl.UserRightsEditServiceImpl"/>
</beans>